    "file-saver": "^2.0.5",
    "localforage": "^1.10.0",
    "react": "^18.2",
    "react-dom": "^18.2",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/react": "^18.2",
    "@types/react-dom": "^18.2",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
